  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug app",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/packages/github_stats_viewer",
      "flutterMode": "debug"
    },
    {
      "name": "Debug server",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/packages/github_authenticator",
      "program": "bin/server.dart"
    },
    {
      "name": "Debug app (wasm)",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/packages/github_stats_viewer",
      "flutterMode": "debug",
      "args": [
        "--wasm",
        "-d",
        "chrome"
      ]
    },
    {
      "name": "Profile app",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/packages/github_stats_viewer",
      "flutterMode": "profile"
    },
    {
      "name": "Release app",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/packages/github_stats_viewer",
      "flutterMode": "release"
    }
  ]
}